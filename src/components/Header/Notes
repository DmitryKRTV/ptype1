let initialScroll = 0;

    let homeElem = 0;
    let aboutElem = 0;
    let skillsElem = 0;
    let servicesElem = 0;
    let contactElem = 0;


    useEffect(() => {
        // @ts-ignore
        initialScroll = document.querySelector("#root").scrollHeight - window.innerHeight;
        // @ts-ignore
        homeElem = document.querySelector(".header").scrollHeight
        // @ts-ignore
        aboutElem = document.querySelector(".about").scrollHeight
        // @ts-ignore
        skillsElem = document.querySelector(".skills").scrollHeight
        // @ts-ignore
        servicesElem = document.querySelector(".services").scrollHeight
        // @ts-ignore
        contactElem = document.querySelector(".contact").scrollHeight
        // @ts-ignore
        console.log(initialScroll)
        // console.log(homeElem)
        // console.log(aboutElem)
        // console.log(skillsElem)
        // console.log(servicesElem)
        // console.log(contactElem)
        document.addEventListener("scroll", scrollHandler)
        return () => {
            document.removeEventListener("scroll",scrollHandler)
        };
    }, []);

    const scrollHandler = (e: any) => {
        console.log(e.target.documentElement.scrollTop)
        const header = document.querySelector(".header")
        if (window.scrollY >= 80 && header) {
            header.classList.add("scroll-header")
        } else if (window.scrollY < 80 && header) {
            header.classList.remove("scroll-header")
        }
        if(window.scrollY === homeElem) {setActiveNav(()=>"#home")}
        if(window.scrollY === aboutElem) {setActiveNav(()=>"#about")}
        if(window.scrollY === skillsElem) {setActiveNav(()=>"#skills")}
        if(window.scrollY === servicesElem) {setActiveNav(()=>"#services")}
        if(window.scrollY === contactElem) {setActiveNav(()=>"#contact")}
    }

    const [toggle, setToggle] = useState<boolean>(false);
    const [activeNav, setActiveNav] = useState<linksType>("#home");
